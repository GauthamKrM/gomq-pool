{
  "title": "Go MQ Consumers",
  "uid": "go-mq-dashboard",
  "schemaVersion": 39,
  "version": 1,
  "templating": {
    "list": [
      {
        "name": "queue",
        "label": "Queue",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
        "query": "label_values(rabbitmq_detailed_queue_messages, queue)",
        "refresh": 1,
        "includeAll": false,
        "multi": false,
        "current": { "text": "test_queue", "value": "test_queue" }
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Total Throughput (success/s)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(consumer_messages_total{result=\"success\",queue=\"$queue\"}[5m]))",
          "legendFormat": "consumer-{{instance}}" 
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Failures and DLQ (events/s)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
        "expr": "sum by (instance) (rate(consumer_messages_total{result=\"failed\",queue=\"$queue\"}[5m]))",
        "legendFormat": "failed-{{instance}}"
        },
        {
        "expr": "sum by (instance) (rate(consumer_dlq_total{queue=\"$queue\"}[5m]))",
        "legendFormat": "dlq-{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Retries (events/s)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum by (instance) (rate(consumer_retries_total{queue=\"$queue\"}[5m]))",
          "legendFormat": "retry-{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Processing Latency P50/P95/P99 (s) - Aggregated",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, priority) (rate(consumer_processing_seconds_bucket{queue=\"$queue\"}[5m])))",
          "legendFormat": "p50-{{priority}}"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, priority) (rate(consumer_processing_seconds_bucket{queue=\"$queue\"}[5m])))",
          "legendFormat": "p95-{{priority}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, priority) (rate(consumer_processing_seconds_bucket{queue=\"$queue\"}[5m])))",
          "legendFormat": "p99-{{priority}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ack/Nack Errors (events/s)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "sum by (instance, type) (rate(consumer_ack_errors_total{queue=\"$queue\"}[5m]))",
          "legendFormat": "{{type}}-{{instance}}"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Main Queue Depth (messages)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "rabbitmq_detailed_queue_messages{queue=\"$queue\"}",
          "legendFormat": "total"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Queue Ready vs Unacked (messages)",
      "datasource": { "type": "prometheus", "uid": "prometheus_ds" },
      "targets": [
        {
          "expr": "rabbitmq_detailed_queue_messages_ready{queue=\"$queue\"}",
          "legendFormat": "ready"
        },
        {
          "expr": "rabbitmq_detailed_queue_messages_unacked{queue=\"$queue\"}",
          "legendFormat": "unacked"
        }
      ]
    }
  ],
  "time": { "from": "now-6h", "to": "now" },
  "tags": ["gomq", "rabbitmq", "prometheus"]
}
